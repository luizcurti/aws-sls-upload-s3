# Example environment configuration
# Copy this file to .env.local for local development

# LocalStack Configuration
DEBUG=1
LOCALSTACK_DOCKER_NAME=localstack-aws-sls-upload-s3
LOCALSTACK_VOLUME_DIR=./tmp/localstack

# AWS Configuration for LocalStack
AWS_ENDPOINT_URL=http://localhost:4566
AWS_DEFAULT_REGION=us-east-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_REGION=us-east-1

# S3 Configuration
S3_BUCKET_NAME=upload-s3-bucket-local
S3_REGION=us-east-1

# Lambda Configuration
LAMBDA_EXECUTOR=local

# Serverless Configuration
SLS_DEBUG=*
IS_LOCAL=true

# Production AWS (do not commit real values)
# AWS_ACCESS_KEY_ID_PROD=your-prod-key
# AWS_SECRET_ACCESS_KEY_PROD=your-prod-secret
# S3_BUCKET_NAME_PROD=your-prod-bucket